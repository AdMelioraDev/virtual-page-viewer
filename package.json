{
  "name": "virtual-page-viewer",
  "version": "0.1.0",
  "description": "A tool for visualizing and debugging VirtualPage pools",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"webpack --config webpack.config.js --watch\" \"electron .\"",
    "build": "webpack --config webpack.config.js",
    "package": "electron-builder"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "concurrently": "^8.0.1",
    "css-loader": "^6.7.3",
    "electron": "^24.1.2",
    "electron-builder": "^23.6.0",
    "style-loader": "^3.3.2",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2"
  },
  "dependencies": {
    "pixi.js": "^7.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "build": {
    "appId": "com.virtual.pageviewer",
    "productName": "VirtualPageViewer",
    "directories": {
      "output": "build"
    },
    "win": {
      "target": "portable"
    }
  }
}
